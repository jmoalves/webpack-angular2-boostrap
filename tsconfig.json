{
    "compilerOptions": {
        "target": "es5",
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es2015", "dom"],
        "suppressImplicitAnyIndexErrors": true
    },
    "include": [
        "./src/**/*"
    ]
}